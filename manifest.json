{
	"manifest_version": 2,
	"name": "RARBG thumbnails",
	"short_name": "RARBG thumbs",
	"description": "Display thumbnails automaticly on RARBG.to",
	"version": "1.0",
	"content_scripts":
	[
		{
			"matches":
			[
				"https://rarbg.to/torrents.php*"
			],
			"js":
			[
				"content.js"
			],
			"run_at": "document_end"
		}
	],
	"icons":
	{
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	}
}